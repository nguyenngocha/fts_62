<% provide :submit, t("subject.edit_subject") %>
<div class="row">
  <div class="col-md-6 panel panel-default">
    <div class="text-center">
      <h3><%= link_to t("subject.edit_subject"), "#" %></h3>
      <hr />
    </div>
    <%= form_for [:admins, @subject] do |f| %>
      <%= render "shared/error_messages", object: @subject %>
      <%= render "form", f: f %>
    <% end %>
  </div>
</div>
